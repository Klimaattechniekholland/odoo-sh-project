<?xml version="1.0"?>
<odoo>
    <record id="view_move_form_custom" model="ir.ui.view">
        <field name="name">account.move.form.custom</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
      <xpath expr="//notebook/page[last()]" position="after">
        <page string="Custom Info">
            <group string="Invoice Infos">
                    <field name="invoice_variant"/>
                    <field name="house_photo" widget="image"/>
                    <field name="system_description"/>
                    <field name="subsidy_amount"/>
                    <field name="meldcode"/>
                    <field name="contract_duration_months"/>
                    <field name="payment_frequency"/>
                    <field name="require_signature"/>
          </group>
          <group string="Selected Kits">
            <field name="kit_bom_ids" widget="many2many_tags"/>
          </group>
        </page>
            </xpath>
        </field>
    </record>
</odoo>